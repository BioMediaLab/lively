{
  "name": "lively-www",
  "version": "0.0.1",
  "description": "The next.js layer for lively.",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "node server.js",
    "gen-types": "cd .. && apollo client:codegen --target typescript",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "typecheck": "yarn gen-types && tsc -p .",
    "watch": "npm-watch"
  },
  "watch": {
    "gen-types": {
      "patterns": [
        "pages",
        "components"
      ],
      "extensions": [
        "ts",
        "tsx"
      ],
      "ignore": [
        "**/__generated__",
        "**/.next"
      ]
    }
  },
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.3.1",
    "apollo-upload-client": "^10.0.0",
    "date-fns": "^1.30.1",
    "encoding": "^0.1.12",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^8.0.3",
    "react": "^16.8.4",
    "react-apollo": "^2.5.2",
    "react-apollo-hooks": "^0.4.4",
    "react-beautiful-dnd": "11.0.0-beta.2",
    "react-dnd": "^7.4.5",
    "react-dnd-html5-backend": "^7.4.4",
    "react-dom": "^16.8.4",
    "react-dropzone": "^10.1.3",
    "react-pdf": "^4.0.5",
    "rebass": "^3.1.0",
    "styled-components": "^4.1.5",
    "typescript": "3.4.3"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^8.1.1",
    "@types/graphql": "^14.2.0",
    "@types/next": "^8.0.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/rebass": "^3.0.3",
    "@types/styled-components": "4.1.5",
    "@types/zeit__next-typescript": "^0.1.2",
    "apollo": "^2.8.2",
    "babel-plugin-styled-components": "^1.10.0",
    "npm-watch": "^0.6.0"
  }
}
